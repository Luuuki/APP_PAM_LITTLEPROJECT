<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/back2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#1E1E1E"
    app:constraint_referenced_tags="@string/back2"
    tools:context=".menu">

    <LinearLayout
        android:id="@+id/lay_6"
        android:layout_width="182dp"
        android:layout_height="168dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="31dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="67dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/lay_5"
        app:layout_constraintTop_toBottomOf="@+id/lay_4">

        <ImageButton
            android:id="@+id/imgbtn_loc"
            android:layout_width="193dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            app:srcCompat="@drawable/placeholder" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/lay_5"
        android:layout_width="182dp"
        android:layout_height="168dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="66dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/lay_6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lay_3">

        <ImageButton
            android:id="@+id/imgbtn_colaboradores"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_weight="1"
            app:srcCompat="@drawable/pessoas" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/lay_4"
        android:layout_width="171dp"
        android:layout_height="165dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="180dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="31dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/lay_6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/lay_3"
        app:layout_constraintTop_toBottomOf="@+id/lay_1" >

        <ImageButton
            android:id="@+id/imgbtn_mansao"
            android:layout_width="match_parent"
            android:layout_height="155dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/mansao" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/lay_1"
        android:layout_width="229dp"
        android:layout_height="118dp"
        android:layout_marginStart="91dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="91dp"
        android:layout_marginBottom="1dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/lay_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="215dp"
            android:layout_height="129dp"
            android:layout_weight="1"
            app:srcCompat="@drawable/logo3" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/lay_3"
        android:layout_width="182dp"
        android:layout_height="168dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="180dp"
        android:layout_marginEnd="15dp"
        android:layout_marginBottom="28dp"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/lay_5"
        app:layout_constraintEnd_toStartOf="@+id/lay_4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lay_1">

        <ImageButton
            android:id="@+id/imgbtn_info"
            android:layout_width="192dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            app:srcCompat="@drawable/sobre"
            tools:ignore="SpeakableTextPresentCheck" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/lay_2"
        android:layout_width="141dp"
        android:layout_height="37dp"
        android:layout_marginStart="140dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="140dp"
        android:layout_marginBottom="102dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toTopOf="@+id/lay_4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lay_1">

        <TextView
            android:id="@+id/txtvw_name2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/txtvw_name2"
            android:textColor="#05BCCF"
            android:textSize="24sp" />
    </LinearLayout>

</android.support.constraint.ConstraintLayout>